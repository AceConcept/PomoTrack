{
  "name": "devtrack",
  "version": "0.12.4",
  "private": true,
  "description": "An app for productive developers",
  "author": {
    "name": "David Thomason",
    "url": "https://davidthomason.com/"
  },
  "scripts": {
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --no-fix",
    "audit:packages": "/bin/bash -c 'yarn audit --groups dependencies; [[ $? -ge 16 ]] && exit 1 || exit 0'",
    "electron:build": "vue-cli-service electron:build --dir",
    "electron:dev": "vue-cli-service electron:serve",
    "electron:release": "vue-cli-service electron:build",
    "electron:serve": "electron dist_electron/bundled",
    "lint:fix": "vue-cli-service lint --fix",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "yarn audit:packages && yarn run lint && yarn run test:unit && yarn run test:gh_pages:build:e2e",
    "cy:open": "cypress open",
    "cy:run": "cypress run -b chrome",
    "test:e2e:dev": "testcafe chrome tests/e2e --selector-timeout 3000 --app \"yarn run web:dev\"",
    "test:e2e:prod": "PORT=5000 testcafe chrome:headless tests/e2e -c 2 --selector-timeout 3000 --app \"yarn run web:serve\"",
    "test:prod:build:e2e": "yarn run web:build && yarn run test:e2e:prod",
    "web:build": "vue-cli-service build --dest dist_web",
    "web:build:analyze": "ANALYZE_WEBPACK=true vue-cli-service build --dest dist_web",
    "web:dev": "vue-cli-service serve",
    "web:prod": "yarn run web:build && yarn run web:serve",
    "web:serve": "node web.js"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "chart.js": "^2.9.4",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-datalabels": "^0.6.0",
    "color-manager": "https://github.com/dtom90/color-manager",
    "core-js": "^3.38.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.3",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.6.5",
    "humanize-duration": "^3.32.1",
    "jquery": "^3.7.1",
    "lodash.clonedeep": "^4.5.0",
    "marked": "^4.0.10",
    "popper.js": "^1.16.0",
    "sass": "^1.80.4",
    "sass-loader": "^9.0.1",
    "vue": "^2.7.16",
    "vue-chartjs": "^3.5.0",
    "vue-color": "^2.7.0",
    "vue-ctk-date-time-picker": "^2.5.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@babel/core": "^7.26.0",
    "@electron/notarize": "^2.5.0",
    "@testing-library/jest-dom": "5.11.9",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-plugin-unit-jest": "^4.5.19",
    "@vue/cli-service": "^4.5.9",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "cypress": "13.16.0",
    "dotenv": "^8.2.0",
    "electron": "^33.2.0",
    "electron-builder": "^23.0.2",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.21.1",
    "moment": "^2.29.4",
    "testcafe": "^1.7.1",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.5",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "resolutions": {
    "moment": ">=2.29.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "displayName": "DevTrack",
  "homepage": "https://github.com/dtom90/DevTrack",
  "repository": "https://github.com/dtom90/DevTrack"
}
